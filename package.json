{
  "name": "hive-io_framework",
  "private": true,
  "scripts": {
    "add": "lerna add --exact",
    "add:dev": "lerna add --dev --exact",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "build:domain": "lerna run build --scope hive-io-domain-example",
    "build:hive": "lerna run build --scope hive-io",
    "build:rest": "lerna run build --scope hive-io-rest-example",
    "precoverage": "del coverage && mkdir coverage",
    "coverage": "lerna run coverage",
    "postcoverage": "bash ./bin/uploadCoverage",
    "dev:container:domain": "docker-compose -f dev/docker/domain/development/docker-compose.yml",
    "dev:container:rest": "docker-compose -f dev/docker/rest/development/docker-compose.yml",
    "dev:package": "lerna run dev --stream",
    "dev:package:domain": "lerna run dev --stream --scope hive-io-domain-example",
    "dev:package:hive": "lerna run dev --stream --scope hive-io",
    "dev:package:rest": "lerna run dev --stream --scope hive-io-rest-example",
    "predocument": "del docs",
    "document": "lerna run document --scope hive-io",
    "initialize": "npx lerna bootstrap && lerna run initialize",
    "link": "lerna link",
    "link:convert": "lerna link convert",
    "test": "lerna run test",
    "test:domain": "lerna run test --scope hive-io-domain-example",
    "test:hive": "lerna run test --scope hive-io",
    "test:rest": "lerna run test --scope hive-io-rest-example",
    "test:example:domain": "docker-compose -f dev/docker/domain/example/docker-compose.yml",
    "test:example:rest": "docker-compose -f dev/docker/rest/example/docker-compose.yml",
    "test:prod:domain": "docker-compose -f dev/docker/domain/production/docker-compose.yml",
    "test:prod:rest": "docker-compose -f dev/docker/rest/production/docker-compose.yml"
  },
  "dependencies": {
    "hive-base": "file:containers/hive-base-js",
    "hive-consumer": "file:containers/hive-consumer-js",
    "hive-io": "file:packages/hive-js",
    "hive-io-domain-example": "file:packages/hive-js-domain-example",
    "hive-io-rest-example": "file:packages/hive-js-rest-example",
    "hive-producer": "file:containers/hive-producer-js",
    "hive-stream-processor": "file:containers/hive-stream-processor-js"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.12.1",
    "@babel/plugin-proposal-optional-chaining": "7.12.7",
    "@babel/plugin-transform-modules-commonjs": "7.12.1",
    "@babel/register": "7.12.10",
    "babel-eslint": "10.1.0",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-plugin-istanbul": "6.0.0",
    "babelify": "10.0.0",
    "browserify": "17.0.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-http": "4.3.0",
    "codecov": "3.8.1",
    "cross-env": "7.0.3",
    "del-cli": "3.0.1",
    "dirty-chai": "2.0.1",
    "jsdoc": "3.6.6",
    "karma": "6.0.0",
    "karma-browserify": "8.0.0",
    "karma-browserstack-launcher": "1.6.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-firefox-launcher": "2.1.0",
    "karma-mocha": "2.0.1",
    "lerna": "3.22.1",
    "minami": "1.2.3",
    "mocha": "8.2.1",
    "nodemon": "2.0.7",
    "nyc": "15.1.0",
    "proxyquire": "2.1.3",
    "sinon": "9.2.3",
    "standard": "16.0.3",
    "terser": "5.5.1",
    "validator": "13.5.2",
    "watchify": "3.11.1"
  }
}
